	ИначеЕсли Свойства.ВидДоступа = ПланыВидовХарактеристик.ВидыДоступа.Магазины Тогда

		Свойства.Таблицы.Добавить("Справочник.Магазины");
		Свойства.ВидДоступаБезГруппЗначенияДоступа            = Истина;
		Свойства.ВидДоступаЕдинственныйДляТипаЗначенияДоступа = Истина;

	КонецЕсли;
	
