		<field xsi:type="DataSetFieldField">
			<dataPath>ПериодЧековККМ</dataPath>
			<field>ПериодЧековККМ</field>
			<useRestriction>
				<condition>true</condition>
			</useRestriction>
			<attributeUseRestriction>
				<field>true</field>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</attributeUseRestriction>
			<presentationExpression>Формат(
		ВЫБОР	КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада)
				ТОГДА
					ВЫБОР	КОГДА День(ПериодЧековККМ) &gt;= 1 И День(ПериодЧековККМ) &lt;= 10 ТОГДА НАЧАЛОПЕРИОДА(ПериодЧековККМ, "Месяц")
							КОГДА День(ПериодЧековККМ) &gt;= 11 И День(ПериодЧековККМ) &lt;= 20 ТОГДА ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ПериодЧековККМ, "Месяц"), "День", 10)
							ИНАЧЕ ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ПериодЧековККМ, "Месяц"), "День", 20)
					КОНЕЦ
				КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя) ТОГДА НАЧАЛОПЕРИОДА(ПериодЧековККМ, "Неделя")
				КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Месяц) ТОГДА НАЧАЛОПЕРИОДА(ПериодЧековККМ, "Месяц")
				КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.День) ТОГДА НАЧАЛОПЕРИОДА(ПериодЧековККМ, "День")
				ИНАЧЕ ДАТАВРЕМЯ(0001,01,01)
		КОНЕЦ, "ДФ=dd.MM.yy")
		+ " - " +
		Формат(
		ВЫБОР	КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада)
				ТОГДА
					ВЫБОР	КОГДА День(ПериодЧековККМ) &gt;= 1 И День(ПериодЧековККМ) &lt;= 10 ТОГДА ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ПериодЧековККМ, "Месяц"), "День", 9)
							КОГДА День(ПериодЧековККМ) &gt;= 11 И День(ПериодЧековККМ) &lt;= 20 ТОГДА ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ПериодЧековККМ, "Месяц"), "День", 19)
							ИНАЧЕ КОНЕЦПЕРИОДА(ПериодЧековККМ, "Месяц")
					КОНЕЦ
				КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя) ТОГДА КОНЕЦПЕРИОДА(ПериодЧековККМ, "Неделя")
				КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Месяц) ТОГДА КОНЕЦПЕРИОДА(ПериодЧековККМ, "Месяц")
				КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.День) ТОГДА КОНЕЦПЕРИОДА(ПериодЧековККМ, "День")
				ИНАЧЕ ДАТАВРЕМЯ(0001,01,01)
		КОНЕЦ, "ДФ=dd.MM.yy")
		+ " "
		+ ВЫБОР	КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада)
				ТОГДА "дек. " + 
					ВЫБОР	КОГДА День(ПериодЧековККМ) &gt;= 1 И День(ПериодЧековККМ) &lt;= 10 ТОГДА "1"
							КОГДА День(ПериодЧековККМ) &gt;= 11 И День(ПериодЧековККМ) &lt;= 20 ТОГДА "2"
							ИНАЧЕ "3"
					КОНЕЦ
				КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя) ТОГДА "нед. " + Строка(Неделя(ПериодЧековККМ))
