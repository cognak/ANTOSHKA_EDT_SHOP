				<dataPath>ПериодРасшифровка</dataPath>
				<field>ПериодРасшифровка</field>
			</field>
			<dataSource>ИсточникДанных1</dataSource>
			<query>// Для расшифровки показателей, ресурсы должны иметь форматную строку в свойстве "Текст"
// Например: ИСТИНА,ИСТИНА,ИСТИНА,Продажи,Динамика доход тек период
// Где:
// ИСТИНА - ПоказательБудетРасшифровывается
// ИСТИНА - ВыводитьДинамику
// ИСТИНА - ВыводитьИнтервалы
// Продажи - ИмяОтчета
// Динамика доход тек период - НаименованиеПоказателя

ВЫБРАТЬ РАЗРЕШЕННЫЕ
	"ЧекиККМ"                 КАК ИдентификаторОтчета,
	ЧекККМ.Дата               КАК Дата,
	ЧекККМ.Ссылка             КАК Ссылка,
	МАКСИМУМ(ЧекККМ.Магазин)  КАК Магазин,
	СУММА(ЧекККМТовары.Сумма) КАК Сумма
ПОМЕСТИТЬ ЧекККМ
ИЗ
	Документ.ЧекККМ КАК ЧекККМ
ВНУТРЕННЕЕ СОЕДИНЕНИЕ
	Документ.ЧекККМ.Товары КАК ЧекККМТовары
ПО
	ЧекККМТовары.Ссылка = ЧекККМ.Ссылка
ГДЕ
	ЧекККМ.Дата МЕЖДУ &amp;ДатаНачалаЧекиККМ И &amp;КонецПериода
	И ЧекККМ.СтатусЧекаККМ = ЗНАЧЕНИЕ(Перечисление.СтатусыЧековККМ.Архивный)
	И ЧекККМ.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийЧекККМ.Продажа)
СГРУППИРОВАТЬ ПО
	ЧекККМ.Ссылка
ИНДЕКСИРОВАТЬ ПО
	Дата
;
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	"ЧекиККМ"                      КАК ИдентификаторОтчета,
	ЧекККМ.Магазин                 КАК Магазин,
	ВЫБОР	КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада)
			ТОГДА	ВЫБОР	КОГДА День(ЧекККМ.Дата) МЕЖДУ 1 И 10 ТОГДА НАЧАЛОПЕРИОДА(ЧекККМ.Дата, Месяц)
							КОГДА День(ЧекККМ.Дата) МЕЖДУ 11 И 20 ТОГДА ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ЧекККМ.Дата, Месяц), День, 10)
							ИНАЧЕ ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ЧекККМ.Дата, Месяц), День, 20)
					КОНЕЦ
			КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Месяц)
			ТОГДА НАЧАЛОПЕРИОДА(ЧекККМ.Дата, Месяц)
			КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя)
			ТОГДА НАЧАЛОПЕРИОДА(ЧекККМ.Дата, Неделя)
			КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.День)
			ТОГДА НАЧАЛОПЕРИОДА(ЧекККМ.Дата, День)
	КОНЕЦ КАК ПериодЧековККМ,
	ВЫБОР	КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада)
			ТОГДА	ВЫБОР	КОГДА День(ЧекККМ.Дата) МЕЖДУ 1 И 10 ТОГДА НАЧАЛОПЕРИОДА(ЧекККМ.Дата, Месяц)
							КОГДА День(ЧекККМ.Дата) МЕЖДУ 11 И 20 ТОГДА ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ЧекККМ.Дата, Месяц), День, 10)
							ИНАЧЕ ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ЧекККМ.Дата, Месяц), День, 20)
					КОНЕЦ
			КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Месяц)
			ТОГДА НАЧАЛОПЕРИОДА(ЧекККМ.Дата, Месяц)
			КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя)
			ТОГДА НАЧАЛОПЕРИОДА(ЧекККМ.Дата, Неделя)
			КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.День)
			ТОГДА НАЧАЛОПЕРИОДА(ЧекККМ.Дата, День)
	КОНЕЦ КАК ПериодРасшифровка,
	ДАТАВРЕМЯ(01,01,0001) КАК СуррогатнаяГруппировка,
	СУММА(ЧекККМ.КоличествоЧеков) КАК КоличествоЧеков,
	СУММА(ЧекККМ.КоличествоЧеков) КАК РасшифровкаКоличествоЧеков,
	ВЫБОР	КОГДА СУММА(ЧекККМ.КоличествоЧеков) = 0.00
			ТОГДА 0.00
			ИНАЧЕ СУММА(ЧекККМ.СуммаЧековВсего) / СУММА(ЧекККМ.КоличествоЧеков)
	КОНЕЦ КАК СредняяСуммаЧеков,
	ВЫБОР	КОГДА СУММА(ЧекККМ.КоличествоЧеков) = 0.00
