				ИНАЧЕ ДАТАВРЕМЯ(0001,01,01)
		КОНЕЦ, "ДФ=dd.MM.yy")
		+ " - " +
		Формат(
		ВЫБОР	КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада)
				ТОГДА
					ВЫБОР	КОГДА День(ПериодРасшифровка) &gt;= 1 И День(ПериодРасшифровка) &lt;= 10 ТОГДА ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ПериодРасшифровка, "Месяц"), "День", 9)
							КОГДА День(ПериодРасшифровка) &gt;= 11 И День(ПериодРасшифровка) &lt;= 20 ТОГДА ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(ПериодРасшифровка, "Месяц"), "День", 19)
							ИНАЧЕ КОНЕЦПЕРИОДА(ПериодРасшифровка, "Месяц")
					КОНЕЦ
				КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя) ТОГДА КОНЕЦПЕРИОДА(ПериодРасшифровка, "Неделя")
				КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Месяц) ТОГДА КОНЕЦПЕРИОДА(ПериодРасшифровка, "Месяц")
				КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.День) ТОГДА КОНЕЦПЕРИОДА(ПериодРасшифровка, "День")
				ИНАЧЕ ДАТАВРЕМЯ(0001,01,01)
		КОНЕЦ, "ДФ=dd.MM.yy")
		+ " "
		+ ВЫБОР	КОГДА &amp;Периодичность = ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада)
				ТОГДА "дек. " + 
					ВЫБОР	КОГДА День(ПериодРасшифровка) &gt;= 1 И День(ПериодРасшифровка) &lt;= 10 ТОГДА "1"
