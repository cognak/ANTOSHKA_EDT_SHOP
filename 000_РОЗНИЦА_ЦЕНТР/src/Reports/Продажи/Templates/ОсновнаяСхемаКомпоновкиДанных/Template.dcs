		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаЧека</dataPath>
			<field>СуммаЧека</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сумма чека</v8:content>
