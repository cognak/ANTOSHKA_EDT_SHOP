					ИНАЧЕ 0
				КОНЕЦ КАК ЧИСЛО(10, 2))),
		СУММА(ВЫБОР
				КОГДА ТабличнаяЧасть.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.БезНДС)
					ТОГДА ТабличнаяЧасть.Сумма
				ИНАЧЕ 0
			КОНЕЦ),
		ТабличнаяЧасть.Ссылка.Магазин,
		NULL,
		NULL,
		NULL,
		NULL,
		NULL,
		ВЫРАЗИТЬ(ВЫБОР
				КОГДА ТабличнаяЧасть.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС14)
					ТОГДА ТабличнаяЧасть.Сумма - ТабличнаяЧасть.Сумма / 114 * 14
				ИНАЧЕ 0
			КОНЕЦ КАК ЧИСЛО(10, 2)),
		NULL
	ИЗ
		Документ.РеализацияТоваров.Товары КАК ТабличнаяЧасть
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.УчетнаяПолитика.СрезПоследних({(&amp;ДатаПо)}, ) КАК УчетнаяПолитика
			ПО ТабличнаяЧасть.Ссылка.Магазин = УчетнаяПолитика.ИнтернетМагазин
	ГДЕ
		ТабличнаяЧасть.Ссылка.Дата МЕЖДУ &amp;ДатаС И &amp;ДатаПо
		И ТабличнаяЧасть.Ссылка.Услуги = ИСТИНА
		И ТабличнаяЧасть.Ссылка.ПометкаУдаления = ЛОЖЬ
		И ТабличнаяЧасть.Ссылка.Проведен = ИСТИНА
		И УчетнаяПолитика.ИнтернетМагазин ЕСТЬ NULL
	
	СГРУППИРОВАТЬ ПО
		НАЧАЛОПЕРИОДА(ТабличнаяЧасть.Ссылка.Дата, ДЕНЬ),
		ТабличнаяЧасть.Ссылка.Магазин,
		ТабличнаяЧасть.Ссылка,
		ВЫРАЗИТЬ(ВЫБОР
				КОГДА ТабличнаяЧасть.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС14)
					ТОГДА ТабличнаяЧасть.Сумма - ТабличнаяЧасть.Сумма / 114 * 14
				ИНАЧЕ 0
			КОНЕЦ КАК ЧИСЛО(10, 2))) КАК ТаблицаПродажи
ГДЕ
	ТаблицаПродажи.Дата МЕЖДУ &amp;ДатаС И &amp;ДатаПо

СГРУППИРОВАТЬ ПО
	ТаблицаПродажи.Магазин,
	ТаблицаПродажи.ДокументПродажи,
	ТаблицаПродажи.Округление,
	ТаблицаПродажи.ОкруглениеВозраты,
	ТаблицаПродажи.Сумма14,
	ТаблицаПродажи.Возвраты14,
	НАЧАЛОПЕРИОДА(ТаблицаПродажи.Дата, ДЕНЬ),
	ТаблицаПродажи.ДокументПродажи.Организация

ИНДЕКСИРОВАТЬ ПО
	ДокументПродажи,
	Магазин
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Продажи.Дата КАК Дата,
	СУММА(Продажи.Сумма20) КАК Сумма20,
	СУММА(Продажи.Сумма7) КАК Сумма7,
	СУММА(Продажи.Сумма0) КАК Сумма0,
	СУММА(Сертификаты.Сумма) КАК СуммаСертификатов,
	Продажи.Магазин КАК Магазин,
	СУММА(Продажи.Возвраты20) КАК Возвраты20,
