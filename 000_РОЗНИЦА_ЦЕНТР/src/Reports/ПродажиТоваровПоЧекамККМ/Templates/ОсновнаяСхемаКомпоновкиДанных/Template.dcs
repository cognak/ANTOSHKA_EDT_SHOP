		<field xsi:type="DataSetFieldField">
			<dataPath>СотрудникМагазин</dataPath>
			<field>СотрудникМагазин</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сотрудники магазина</v8:content>
				</v8:item>
