    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Список.Ссылка</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ТаблицаСправочник.Ссылка КАК Ссылка,
	ТаблицаСправочник.ПометкаУдаления КАК ПометкаУдаления,
	ТаблицаСправочник.Предопределенный КАК Предопределенный,
	ТаблицаСправочник.Родитель КАК Родитель,
	ТаблицаСправочник.ЭтоГруппа КАК ЭтоГруппа,
	ТаблицаСправочник.Код КАК Код,
	ТаблицаСправочник.Наименование КАК Наименование,
	ВЫРАЗИТЬ(ТаблицаСправочник.НаименованиеПолное КАК СТРОКА(500)) КАК НаименованиеПолное,
	ВЫРАЗИТЬ(ТаблицаСправочник.Комментарий КАК СТРОКА(500)) КАК Комментарий,
	ВЫРАЗИТЬ(ТаблицаСправочник.ДокументУдостоверяющийЛичность КАК СТРОКА(500)) КАК ДокументУдостоверяющийЛичность,
	ТаблицаСправочник.КодПоЕДРПОУ КАК КодПоЕДРПОУ,
	ТаблицаСправочник.ЮрФизЛицо КАК ЮрФизЛицо,
	ТаблицаСправочник.ПлательщикНДС КАК ПлательщикНДС,
	ТаблицаСправочник.НомерСвидетельстваПлательщикаНДС КАК НомерСвидетельстваПлательщикаНДС,
	ТаблицаСправочник.ИННПлательщикаНДС КАК ИННПлательщикаНДС,
	ТаблицаСправочник.ДатаРождения КАК ДатаРождения,
	ТаблицаСправочник.Покупатель КАК Покупатель,
	ТаблицаСправочник.Поставщик КАК Поставщик,
	ТаблицаСправочник.Производитель КАК Производитель,
	ТаблицаСправочник.Пол КАК Пол,
	ТаблицаСправочник.IDN КАК IDN,
	ТаблицаСправочник.Автор КАК Автор,
	ТаблицаСправочник.УзелСоздания КАК УзелСоздания,
	ТаблицаСправочник.РазрешитьПрямуюЗакупку КАК РазрешитьПрямуюЗакупку,
	СУММА(ЕСТЬNULL(ТаблицаНакоплений.СуммаОборот, 0)) КАК СуммаНакоплено,
	ВЫБОР
		КОГДА МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон1.Представление, &quot;&quot;)) &lt;> &quot;&quot;
				И МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон2.Представление, &quot;&quot;)) &lt;> &quot;&quot;
			ТОГДА МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон1.Представление, &quot;&quot;)) + &quot;, &quot; + МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон2.Представление, &quot;&quot;))
		КОГДА МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон1.Представление, &quot;&quot;)) = &quot;&quot;
			ТОГДА МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон2.Представление, &quot;&quot;))
		ИНАЧЕ МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон1.Представление, &quot;&quot;))
	КОНЕЦ КАК НомерТелефонаПолный,
	ВЫБОР
		КОГДА МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон1.НомерТелефонаБезКодов, &quot;&quot;)) &lt;> &quot;&quot;
				И МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон2.НомерТелефонаБезКодов, &quot;&quot;)) &lt;> &quot;&quot;
			ТОГДА МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон1.НомерТелефонаБезКодов, &quot;&quot;)) + &quot;, &quot; + МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон2.НомерТелефонаБезКодов, &quot;&quot;))
		КОГДА МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон1.НомерТелефонаБезКодов, &quot;&quot;)) = &quot;&quot;
			ТОГДА МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон2.НомерТелефонаБезКодов, &quot;&quot;))
		ИНАЧЕ МАКСИМУМ(ЕСТЬNULL(КонтрагентыТелефон1.НомерТелефонаБезКодов, &quot;&quot;))
	КОНЕЦ КАК НомерТелефона,
	ВЫБОР
		КОГДА МАКСИМУМ(ЕСТЬNULL(EmailКонтрагента1.АдресЭП, &quot;&quot;)) &lt;> &quot;&quot;
				И МАКСИМУМ(ЕСТЬNULL(EmailКонтрагента2.АдресЭП, &quot;&quot;)) &lt;> &quot;&quot;
			ТОГДА МАКСИМУМ(ЕСТЬNULL(EmailКонтрагента1.АдресЭП, &quot;&quot;)) + &quot;, &quot; + МАКСИМУМ(ЕСТЬNULL(EmailКонтрагента2.АдресЭП, &quot;&quot;))
		КОГДА МАКСИМУМ(ЕСТЬNULL(EmailКонтрагента1.АдресЭП, &quot;&quot;)) = &quot;&quot;
			ТОГДА МАКСИМУМ(ЕСТЬNULL(EmailКонтрагента2.АдресЭП, &quot;&quot;))
		ИНАЧЕ МАКСИМУМ(ЕСТЬNULL(EmailКонтрагента1.АдресЭП, &quot;&quot;))
	КОНЕЦ КАК Email
ИЗ
	Справочник.Контрагенты КАК ТаблицаСправочник
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ПродажиПоДисконтнымКартам.Обороты(, , , ВладелецДисконтнойКарты ССЫЛКА Справочник.Контрагенты) КАК ТаблицаНакоплений
		ПО ТаблицаСправочник.Ссылка = ТаблицаНакоплений.ВладелецДисконтнойКарты
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Контрагенты.КонтактнаяИнформация КАК КонтрагентыТелефон1
		ПО (КонтрагентыТелефон1.Ссылка = ТаблицаСправочник.Ссылка)
			И (КонтрагентыТелефон1.Вид = ЗНАЧЕНИЕ(Справочник.ВидыКонтактнойИнформации.ТелефонКонтрагента))
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Контрагенты.КонтактнаяИнформация КАК КонтрагентыТелефон2
		ПО (КонтрагентыТелефон2.Ссылка = ТаблицаСправочник.Ссылка)
			И (КонтрагентыТелефон2.Вид = ЗНАЧЕНИЕ(Справочник.ВидыКонтактнойИнформации.ТелефонКонтрагентаСлужебный))
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Контрагенты.КонтактнаяИнформация КАК EmailКонтрагента1
		ПО (EmailКонтрагента1.Ссылка = ТаблицаСправочник.Ссылка)
			И (EmailКонтрагента1.Вид = ЗНАЧЕНИЕ(Справочник.ВидыКонтактнойИнформации.EmailКонтрагента))
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Контрагенты.КонтактнаяИнформация КАК EmailКонтрагента2
		ПО (EmailКонтрагента2.Ссылка = ТаблицаСправочник.Ссылка)
			И (EmailКонтрагента2.Вид = ЗНАЧЕНИЕ(Справочник.ВидыКонтактнойИнформации.EmailКонтрагентаСлужебный))

СГРУППИРОВАТЬ ПО
	ТаблицаСправочник.Ссылка,
	ТаблицаСправочник.ПометкаУдаления,
	ТаблицаСправочник.Предопределенный,
	ТаблицаСправочник.Родитель,
	ТаблицаСправочник.ЭтоГруппа,
	ТаблицаСправочник.Код,
	ТаблицаСправочник.Наименование,
	ТаблицаСправочник.КодПоЕДРПОУ,
	ТаблицаСправочник.ЮрФизЛицо,
	ТаблицаСправочник.ПлательщикНДС,
	ТаблицаСправочник.НомерСвидетельстваПлательщикаНДС,
	ТаблицаСправочник.ИННПлательщикаНДС,
	ТаблицаСправочник.ДатаРождения,
	ТаблицаСправочник.Покупатель,
	ТаблицаСправочник.Поставщик,
	ТаблицаСправочник.Пол,
	ТаблицаСправочник.IDN,
	ТаблицаСправочник.РазрешитьПрямуюЗакупку,
	ВЫРАЗИТЬ(ТаблицаСправочник.НаименованиеПолное КАК СТРОКА(500)),
	ВЫРАЗИТЬ(ТаблицаСправочник.Комментарий КАК СТРОКА(500)),
	ВЫРАЗИТЬ(ТаблицаСправочник.ДокументУдостоверяющийЛичность КАК СТРОКА(500)),
	ТаблицаСправочник.Производитель,
	ТаблицаСправочник.УзелСоздания,
	ТаблицаСправочник.Автор</queryText>
      <mainTable>Catalog.Контрагенты</mainTable>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
